<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootManageCustomer" prefHeight="350.0" prefWidth="450.0" style="-fx-background-color: #1e272e;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewController.adminController.CustomerManageController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="164.0" layoutY="27.0" text="Manage Customer " textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="372.0" layoutY="293.0" mnemonicParsing="false" onAction="#btnAddAction" prefHeight="25.0" prefWidth="55.0" style="-fx-background-color: #0fbcf9;" text="Add" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="219.0" layoutY="293.0" mnemonicParsing="false" onAction="#btnClearAction" prefHeight="25.0" prefWidth="55.0" style="-fx-background-color: #ef5777;" text="Clear" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TableView fx:id="tableCars" layoutX="17.0" layoutY="56.0" onMouseClicked="#mouseClickedCustomerTable" prefHeight="200.0" prefWidth="417.0">
        <columns>
          <TableColumn prefWidth="71.0" text="Cust ID" />
            <TableColumn prefWidth="78.0" text="Cust Name" />
            <TableColumn prefWidth="68.0" text="NIC" />
            <TableColumn prefWidth="111.0" text="Address" />
            <TableColumn prefWidth="88.0" text="Phone number" />
        </columns>
      </TableView>
      <Button layoutX="296.0" layoutY="293.0" mnemonicParsing="false" onAction="#btnUpdateAction" style="-fx-background-color: #0be881;" text="Update" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
